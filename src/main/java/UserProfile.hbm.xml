<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.saints.working.UserProfile" table="UserProfile">
		<id name="volunteerID" column="volunteerID" type="int">
			<generator class="increment"></generator>
		</id>
		
		<property name="userFirstName" column="userFirstName" type="int"></property>
		<property name="userLastName" column="userLastName" type="string"></property>
		<property name="signInName" column="signInName" type="string"></property>
		<property name="password" column="password" type="string"></property>
		<property name="address" column="address" type="string"></property>
		<property name="phoneNumber" column="phoneNumber" type="string"></property>
		<property name="email" column="email" type="String"></property>
		
	</class>
</hibernate-mapping>